{
    "name": "revinate/rabbitmq-bundle",
    "description": "Revinate RabbitMQ bundle which works with Symfony 2",
    "keywords": ["symfony2", "rabbitmq", "message", "queue"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [{
        "name" : "Vinay Kumar"
    }],
    "require": {
        "php"                         : ">=5.3.0",
        "symfony/yaml"                : ">=2.4.8",
        "php-amqplib/php-amqplib"     : ">=2.7.2",
        "symfony/dependency-injection": ">=2.4.8",
        "symfony/config"              : ">=2.4.8"
    },
    "require-dev": {
      "symfony/console": ">=2.4.8",
      "symfony/framework-bundle": ">=2.4.8",
      "phpunit/phpunit": ">=4.6.9",
      "symfony/finder": ">=2.4.8",
      "symfony/process": ">=2.4.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": {
            "Revinate\\RabbitMqBundle\\": ""
        }
    },
    "autoload-dev": {
      "psr-0": {
        "Revinate\\RabbitMqBundle\\Test\\": "Test/app"
      }
    },
  "target-dir" : "Revinate/RabbitMqBundle"
}
